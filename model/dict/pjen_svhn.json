{
    "name": "ProjEncoder-SVHN",
    "nnet_seq":{
        "conv1":{
            "type": "Conv2d",
            "params":{
                "in_channels": 3,
                "out_channels": 32,
                "kernel_size": 5,
                "padding": 0
            }
        },
        "pool1":{
            "type": "MaxPool2d",
            "params":{
                "kernel_size": 2
            }
        },
        "relu1":{
            "type": "ReLU",
            "params":{
                "inplace": false
            }
        },
        "conv2":{
            "type": "Conv2d",
            "params":{
                "in_channels": 32,
                "out_channels": 32,
                "kernel_size": 3,
                "padding": 1
            }
        },
        "drop1":{
            "type": "Dropout2d",
            "params":{
                "p": 0.1
            }
        },
        "pool2":{
            "type": "MaxPool2d",
            "params":{
                "kernel_size": 2
            }
        },
        "relu2":{
            "type": "ReLU",
            "params":{
                "inplace": false
            }
        }
    }
}